<==> arguments
--migrate-deps --remove-prefix=lib- --forward=prefixed

<==> input/entrypoint.scss
@import "lib1";
@import "lib2";
@import "lib3";

<==> input/_lib1.scss
@function lib-fn() {
  @return 0;
}

@mixin lib-mixin {
  a: b;
}

<==> input/_lib2.scss
$lib-a: 1;
$lib-b: 2;
$c: 3;

<==> input/_lib3.scss
$lib-d: 4;
$e: 5;
$f: 6;

<==> output/entrypoint.scss
@forward "lib1";
@forward "lib2" hide $c;
@forward "lib3" show $d;
@use "lib1";
@use "lib2";
@use "lib3";

<==> output/_lib1.scss
@function fn() {
  @return 0;
}

@mixin mixin {
  a: b;
}

<==> output/_lib2.scss
$a: 1;
$b: 2;
$c: 3;

<==> output/_lib3.scss
$d: 4;
$e: 5;
$f: 6;
