<==> arguments
--migrate-deps --forward=import-only

<==> input/entrypoint.scss
@import "midstream";

<==> input/_midstream.scss
@forward "upstream" as p1-*;
@forward "upstream" as p2-*;

<==> input/_upstream.scss
$a: a;

<==> output/entrypoint.scss
@use "midstream";

<==> output/entrypoint.import.scss
@forward "midstream";
@forward "entrypoint";
