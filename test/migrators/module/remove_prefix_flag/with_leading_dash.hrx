<==> arguments
--remove-prefix=--lib-

<==> input/entrypoint.scss
$--lib-a: 5;
$--lib_b: $--lib-a + 2;

@function fn($--lib-local) {
  @return $--lib-local;
}

--lib-selector {
  property: fn(0);
  --lib-property: --lib-value;
}

<==> output/entrypoint.scss
$a: 5;
$b: $a + 2;

@function fn($--lib-local) {
  @return $--lib-local;
}

--lib-selector {
  property: fn(0);
  --lib-property: --lib-value;
}
