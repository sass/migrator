<==> input/entrypoint.scss
$b: 10;
$c: 1;
$d: 5;

// Single interpolation
.a { .b: calc($b * #{$c + 1}); }

// More than one interpolations
.a { 
    .b: calc($b - #{$c + 1} + #{$d}); 
    .c: calc(100% - #{$table_title_height + 2px});
}

// Nested
.a { .b: calc(3 + max(#{$c, 2})); }

// Nested and more interpolations
.a { .b: calc(#{$b} + max(#{$c, 2})); }

// CSS Custom properties keep interpolation
.a { --test: calc(#{$b} + 1);}

